# Source OpenAI secrets
source_env ~/.secrets/openai.env

# Map OPENAI_BEAKYN_JP_KEY to opencommit's expected variable
export OCO_OPENAI_API_KEY="${OPENAI_BEAKYN_JP_KEY}"

# Default OpenAI
export OPENAI_API_KEY="${OPENAI_BEAKYN_JP_KEY}"

# aicommit2 per-project config
export AICOMMIT_CONFIG_PATH="${PWD}/.aicommit2/config.ini"

